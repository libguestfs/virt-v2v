#. type: =item
#: ../docs/virt-v2v.pod:1627 ../virt-v2v-1.41.8/docs/virt-v2v.pod:1627
msgid "F</usr/share/virtio-win/virtio-win.iso>"
msgstr ""

#. type: textblock
#: ../docs/virt-v2v.pod:1629 ../virt-v2v-1.41.8/docs/virt-v2v.pod:1629
msgid "The ISO containing virtio drivers for Windows."
msgstr ""

#. type: textblock
#: ../docs/virt-v2v.pod:1633 ../virt-v2v-1.41.8/docs/virt-v2v.pod:1633
msgid ""
"The exploded tree of virtio drivers for Windows.  This is usually "
"incomplete, hence the ISO is preferred."
msgstr ""

#. type: textblock
#: ../docs/virt-v2v.pod:1638 ../virt-v2v-1.41.8/docs/virt-v2v.pod:1638
msgid ""
"(F<> if unset).  It can be a directory I<or> point to F<virtio-win.iso> (CD "
"ROM image containing drivers)."
msgstr ""

#. type: textblock
#: ../docs/virt-v2v.pod:1641 ../virt-v2v-1.41.8/docs/virt-v2v.pod:1641
msgid "See L</Enabling virtio>."
msgstr ""

#. type: textblock
#: ../docs/virt-v2v.pod:1645 ../virt-v2v-1.41.8/docs/virt-v2v.pod:1645
msgid ""
"For other environment variables, see L<guestfs(3)/ENVIRONMENT VARIABLES>."
msgstr ""

#. type: =head1
#: ../docs/virt-v2v.pod:1647 ../virt-v2v-1.41.8/docs/virt-v2v.pod:1647
msgid "OTHER TOOLS"
msgstr ""

#. type: =item
#: ../docs/virt-v2v.pod:1651 ../virt-v2v-1.41.8/docs/virt-v2v.pod:1651
msgid "L<virt-v2v-copy-to-local(1)>"
